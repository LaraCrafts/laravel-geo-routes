{
    "name": "laracrafts/laravel-geo-routes",
    "description": "Geo location restricted routes for Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Choraimy Kroonstuiver",
            "email": "choraimy@live.nl"
        },
        {
            "name": "Raed Yak",
            "email": "contact.raedyak@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "laravel/framework": "^5.1",
        "stevebauman/location": "^3.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.14",
        "mockery/mockery": "^1.2",
        "orchestra/testbench": "*",
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "LaraCrafts\\GeoRoutes\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaraCrafts\\GeoRoutes\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "laravel": {
            "providers": [
                "\\LaraCrafts\\GeoRoutes\\GeoRoutesServiceProvider"
            ]
        }
    }
}
